BOT_TOKEN=your_telegram_bot_token
ADMIN_CHAT_ID=123456789

VFS_EMAIL=vfs_login@email.com
VFS_PASSWORD=vfs_password

CHECK_INTERVAL=120
CHECK_INTERVAL_VARIATION=30

TARGET_MONTH=3
TARGET_DAYS=15,16,17
TARGET_DAYS_OF_WEEK=1,2,3,4,5

TARGET_TIME_START=07:00
TARGET_TIME_END=22:00

# Минимальный вариант: бот подключается к Chrome с отладкой (обход антибота VFS).
CHROME_CDP_URL=http://127.0.0.1:9222
# Если задать CHROME_LAUNCH_CDP=1, бот сам запустит Chrome с отладкой (не нужно запускать вручную).
CHROME_LAUNCH_CDP=1
# Путь к Chrome, если нестандартный (папка или полный путь к chrome.exe):
# CHROME_PATH=D:\Program Files (x86)\Google\Chrome\Application

# Режим ручного логина: бот не вводит логин/пароль, а использует уже авторизованный браузер.
# VFS_MANUAL_LOGIN=1
